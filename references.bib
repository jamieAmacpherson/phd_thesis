	Mesh = {Animals; Cell Proliferation; Glucose; Glutamine; Humans; Metabolic Networks and Pathways; Pyruvate Kinase; Serine},
	Pages = {325-34},
	Pmid = {22262476},
	Pst = {ppublish},
	Title = {Metabolic pathway alterations that support cell proliferation},
	Volume = {76},
