	Journal = {Biochemistry},
	Journal-Full = {Biochemistry},
	Mesh = {Allosteric Regulation; Allosteric Site; Animals; Catalysis; Crystallography, X-Ray; Enzyme Reactivators; Enzyme Stability; Fructosediphosphates; Humans; Kinetics; Ligands; Models, Molecular; Neoplasm Proteins; Oxalic Acid; Protein Conformation; Protein Structure, Tertiary; Pyruvate Kinase; Rabbits; Saccharomyces cerevisiae Proteins; Substrate Specificity; Thermodynamics},
	Month = {Jul},
