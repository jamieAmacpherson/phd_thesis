	Date-Modified = {2016-06-12 11:32:59 +0000},
	Journal = {Structure},
	Journal-Full = {Structure (London, England : 1993)},
	Mesh = {Allosteric Regulation; Alternative Splicing; Amino Acid Sequence; Binding Sites; Fructosediphosphates; Genes; Glycolates; Manganese; Models, Molecular; Molecular Sequence Data; Phosphoenolpyruvate; Potassium; Protein Conformation; Pyruvate Kinase; Saccharomyces cerevisiae},
	Month = {Feb},
