	Doi = {10.1002/humu.23231},
	Journal = {Hum Mutat},
	Journal-Full = {Human mutation},
	Keywords = {PKLR; alanine-scanning mutagenesis; allosteric mechanism; allosteric regulation; allostery; liver pyruvate kinase},
	Mesh = {Alanine; Algorithms; Allosteric Regulation; Computational Biology; Crystallography, X-Ray; Enzyme Activation; Fructosediphosphates; Humans; Models, Molecular; Mutation; Pyruvate Kinase},
	Month = {Sep},
	Number = {9},
