	Mesh = {Cell Line; Enzyme Inhibitors; Fructosediphosphates; Glucose; Glycolysis; High-Throughput Screening Assays; Humans; Isoenzymes; L-Lactate Dehydrogenase; Pyruvate Kinase},
	Month = {Apr},
