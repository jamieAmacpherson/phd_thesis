	Pmid = {11326672},
	Pst = {ppublish},
	Title = {Tumor type M2 pyruvate kinase expression in advanced breast cancer},
	Volume = {20},
	Year = {2000}}

@article{Lu:2016ac,
	Abstract = {Recently, pyruvate kinase M2 ({PKM2}) has been implicated in the progression of certain cancers and might play pivotal roles in the formation of malignancy. However, the role of {PKM2} in gallbladder cancer had not been well investigated. This study analyzed associations between {PKM2} expression status with various clinical and pathologic parameters in a large cohort of gallbladder cancer (GBC) patients from a long term follow up results. The expression level of pyruvate kinase isotypes in GBC tissues and their adjacent normal gallbladder tissues were estimated by qRT-PCR and Western blot. {PKM2} mRNA level were significantly high in gallbladder cancer tissues than in adjacent noncancerous tissues (P < 0.001). High expression of the {PKM2} was detected in 55.71% paraffin-embedded GBC tissue. The high {PKM2} expression was independently associated with poorer overall survival in patients with GBC (median survival 11.9 vs 30.1 months; hazard ratio 2.79; 95% CI = 1.18 to 6.55; P = 0.02). These findings indicated elevated expression of {PKM2} is a prognostic factor for poor GBC clinical outcomes, implied involving of {PKM2} in GBC progression.},
	Author = {Lu, Wei and Cao, Yang and Zhang, Yijian and Li, Sheng and Gao, Jian and Wang, Xu-An and Mu, Jiasheng and Hu, Yun-Ping and Jiang, Lin and Dong, Ping and Gong, Wei and Liu, Yingbin},
