	Mesh = {Binding Sites; Catalysis; Cyclophilin A; Models, Molecular; Movement; Pliability; Point Mutation; Protein Conformation; Structure-Activity Relationship},
	Month = {Nov},
	Number = {7064},
	Pages = {117-21},
