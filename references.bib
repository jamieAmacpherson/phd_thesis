	Journal = {J Biol Chem},
	Journal-Full = {The Journal of biological chemistry},
	Keywords = {Escherichia coli (E. coli); allosteric regulation; conformational flexibility; fructose 1,6-bisphosphate; glycolysis; hydrogen-deuterium exchange; pyruvate kinase},
	Month = {Apr},
