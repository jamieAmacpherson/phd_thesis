	Keywords = {O-GlcNAcylation; {PKM2}; Warburg effect; cancer metabolism},
	Mesh = {Acylation; Cell Line; Female; Glucose; Humans; Lactic Acid; Male; Neoplasm Proteins; Neoplasms; Protein Processing, Post-Translational; Pyruvate Kinase},
