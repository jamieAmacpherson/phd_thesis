	Mesh = {Adaptation, Physiological; Amino Acid Sequence; Amino Acid Substitution; Binding Sites; Evolution, Molecular; Ligands; Models, Molecular; Molecular Sequence Data; Mutant Proteins; Mutation; PDZ Domains; Proteins},
	Month = {Nov},
	Number = {7422},
