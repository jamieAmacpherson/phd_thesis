	Pages = {590-605},
	Pmid = {26365176},
	Pst = {ppublish},
	Title = {MYC/PGC-1α Balance Determines the Metabolic Phenotype and Plasticity of Pancreatic Cancer Stem Cells},
	Volume = {22},
