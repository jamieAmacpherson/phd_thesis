	Doi = {10.1016/j.jmgm.2012.12.011},
	Journal = {J Mol Graph Model},
	Journal-Full = {Journal of molecular graphics \& modelling},
	Mesh = {Allosteric Regulation; Allosteric Site; Amino Acids; Hydrogen Bonding; Hydrophobic and Hydrophilic Interactions; Protein Binding; Protein Structure, Secondary; Proteins},
	Month = {Mar},
	Pages = {30-9},
	Pmid = {23337573},
	Pst = {ppublish},
	Title = {Toward an understanding of the sequence and structural basis of allosteric proteins},
	Volume = {40},
