	Mesh = {Allosteric Regulation; Allosteric Site; Computational Biology; Ligands; Models, Molecular; Models, Statistical; Mutation; Protein Binding; Proteins; Signal Transduction},
	Month = {Dec},
	Number = {24},
	Pages = {3996-3998},
