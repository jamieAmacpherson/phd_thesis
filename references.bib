	Doi = {10.1098/rsos.140120},
	Journal = {R Soc Open Sci},
	Journal-Full = {Royal Society open science},
	Keywords = {Leishmania; Trypanosoma; apoenzyme; effector bound enzyme; specificity constant; tetramer stability},
