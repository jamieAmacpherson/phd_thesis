	Pmid = {27481063},
	Pst = {ppublish},
	Title = {Succinyl-5-aminoimidazole-4-carboxamide-1-ribose 5'-Phosphate (SAICAR) Activates Pyruvate Kinase Isoform M2 ({PKM2}) in Its Dimeric Form},
