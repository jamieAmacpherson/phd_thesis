	Mesh = {Computational Biology; Fructosediphosphates; Glucose; Glucose Transport Proteins, Facilitative; Glyceraldehyde-3-Phosphate Dehydrogenase (Phosphorylating); Glycolysis; HCT116 Cells; Humans; L-Lactate Dehydrogenase; Lactic Acid; Mass Spectrometry; Metabolomics; Models, Chemical; Monte Carlo Method; Phenotype; Phosphorylation},
	Month = {Jul},
