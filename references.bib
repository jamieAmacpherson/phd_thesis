	Journal-Full = {Molecular \& cellular proteomics : MCP},
	Mesh = {Animals; Arabidopsis; Bacteria; Bacterial Proteins; Databases, Protein; Humans; Internet; Plant Proteins; Proteome; Proteomics; Saccharomyces cerevisiae; Saccharomyces cerevisiae Proteins; Species Specificity; Tandem Mass Spectrometry; Transcriptome},
	Month = {Aug},
	Number = {8},
