	Journal = {Exp Mol Pathol},
	Journal-Full = {Experimental and molecular pathology},
	Mesh = {Animals; Antineoplastic Agents; Enzyme Inhibitors; Genes, ras; Glucose; Glycolysis; Humans; Neoplasms; Phosphofructokinase-2; Transformation, Genetic},
