	Mesh = {Algorithms; Amino Acid Sequence; Bacterial Proteins; Computer Simulation; Crystallography, X-Ray; Evolution, Molecular; Flagella; Models, Genetic; Models, Molecular; Mutation; Phylogeny; Protein Binding; Protein Interaction Maps; Protein Structure, Secondary; Protein Structure, Tertiary; Salmonella},
	Number = {11},
